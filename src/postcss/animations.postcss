@keyframes pulse {
    45% {
        transform: scale(1);
    }
    65% {
        transform: scale(1.05);
    }
    100% {
        transform: scale(1);
    }
}
@keyframes elastic {
    45% {
        transform: rotate(0);
    }
    55% {
        transform: rotateY(-5deg);
    }
    75% {
        transform: rotateY(15deg);
    }
    85% {
        transform: rotateY(-5deg);
    }
    100% {
        transform: rotate(0);
    }
}
